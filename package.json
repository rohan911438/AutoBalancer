{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:indexer\"",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && npm run dev",
    "dev:indexer": "cd explorer && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:dev": "vite build --mode development",
    "build:frontend": "vite build",
    "build:backend": "cd backend && npm ci --include=dev && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:indexer\"",
    "start:backend": "cd backend && npm start",
    "start:indexer": "cd explorer && npm start",
    "setup": "npm run setup:deps && npm run setup:env && npm run setup:db",
    "setup:deps": "npm install && cd backend && npm install && cd explorer && npm install",
    "setup:env": "node scripts/setup-environment.js",
    "setup:db": "cd backend && npm run db:setup",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "vitest run",
    "test:backend": "cd backend && npm test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "validate": "npm run validate:frontend && npm run validate:backend && npm run validate:indexer",
    "validate:frontend": "tsc --noEmit",
    "validate:backend": "cd backend && npm run validate",
    "validate:indexer": "cd explorer && npm run validate",
    "clean": "rimraf dist backend/dist explorer/generated && npm run clean:deps",
    "clean:deps": "rimraf node_modules backend/node_modules explorer/node_modules",
    "preview": "vite preview",
    "workflow:check": "node scripts/check-workflow.js",
    "workflow:start": "node scripts/start-workflow.js",
    "workflow:stop": "node scripts/stop-workflow.js"
  },
