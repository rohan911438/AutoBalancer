{
  "name": "explorer",
  "version": "0.1.0",
  "description": "Envio indexer for AutoBalancer Agent contract explorer",
  "main": "src/EventHandlers.ts",
  "scripts": {
    "dev": "envio dev",
    "start": "envio start",
    "stop": "envio stop",
    "codegen": "envio codegen",
    "build": "envio build",
    "test": "envio test",
    "local": "envio local docker up",
    "local:down": "envio local docker down",
    "validate": "npm run codegen && npx tsc --noEmit --skipLibCheck",
    "clean": "rm -rf generated && rm -rf .envio",
    "logs": "docker logs envio-hasura",
    "db:reset": "envio local db reset",
    "setup": "npm run codegen",
    "health": "curl -f http://localhost:8080/healthz || exit 1"
  },
  "keywords": [
    "envio",
    "indexer",
    "autobalancer",
    "ethereum",
    "explorer"
  ],
  "author": "AutoBalancer Team",
  "license": "MIT",
  "dependencies": {
    "envio": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}